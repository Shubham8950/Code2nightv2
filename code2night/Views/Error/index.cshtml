@model System.Web.Mvc.HandleErrorInfo
@{
    ViewBag.Title = "Oops Page Not Found ! | code2night.com";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Error</title>
</head>
<body>
    <div class="page-wrapper">
        <div class="page-intro">
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <ol class="breadcrumb">
                            <li><i class="fa fa-home pr-10"></i><a href="@Url.Action("Index","Users")">Home</a></li>
                            <li class="active">Error 404</li>
                        </ol>
                    </div>
                </div>
            </div>
        </div>
        <section class="main-container">
            <div class="container">
                <div class="row">
                   <div class="main col-md-6 col-md-offset-3">
                        <h1 class="title">Ooops! Page Not Found - 404</h1>
                        <br>
                        <p>The requested URL was not found on this server. Make sure that the Web site address displayed in the address bar of your browser is spelled and formatted correctly or <a href="@Url.Action("Index","Users")">return to Home page.</a></p>
                        <div class="block clearfix" style="padding-bottom:0px !important ">
                            <script async src="https://cse.google.com/cse.js?cx=54fa958b97e2934d7"></script>
                            <label>Improve your Search at Code2night.com</label>
                            <div class="gcse-search"></div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
     </div>
</body>
</html>
